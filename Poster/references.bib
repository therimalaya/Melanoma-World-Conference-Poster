@article{robsahm_sex_2013,
  title = {Sex Differences in Rising Trends of Cutaneous Malignant Melanoma in {{Norway}}, 1954-2008},
  author = {Robsahm, Trude E. and Bergva, Gjøril and Hestvik, Unn E. and Møller, Bjørn},
