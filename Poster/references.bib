  shorttitle = {Trends in Net Survival from Skin Malignant Melanoma in Six {{European Latin}} Countries},
  author = {Galceran, Jaume and Uhry, Zoé and Marcos-Gragera, Rafael and Borràs, Joan and Group, the GRELL EUROCARE-5 Working},
  date = {2017-01},
  journaltitle = {European Journal of Cancer Prevention},
  volume = {26},
