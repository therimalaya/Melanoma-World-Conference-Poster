@article{elwood_situ_2019,
  title = {In Situ and Invasive Melanoma in a High-Risk, {{New Zealand}}, Population: {{A}} Population-Based Study},
  shorttitle = {In Situ and Invasive Melanoma in a High-Risk, {{New Zealand}}, Population},
  author = {Elwood, J. Mark and Kim, Stella J.-H. and Ip, Ken H.-K. and Oakley, Amanda and Rademaker, Marius},
